{
    "name": "fazland/notifire-bundle",
    "require": {
        "php": ">= 5.5",
        "fazland/notifire": "^0.3.1",
        "symfony/framework-bundle": "^2.7|^3.0"
    },
    "require-dev": {
        "fazland/skebby-rest-client": "^0.1",
        "kcs/function-mock": "dev-master",
        "mailgun/mailgun-php": "^2.0",
        "phpunit/phpunit": "^4.8|^5.0",
        "symfony/browser-kit": "^2.7|^3.0",
        "symfony/css-selector": "^2.3|^3.0",
        "symfony/swiftmailer-bundle": "^2.3.3",
        "symfony/twig-bundle": "^2.7|^3.0",
        "twilio/sdk": "^4.10",
        "sendgrid/sendgrid": "^5.1"
    },
    "conflict": {
        "symfony/swiftmailer-bundle": "<2.3.3",
        "symfony/dependency-injection": "<2.7.1"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\NotifireBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\NotifireBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable"
}
